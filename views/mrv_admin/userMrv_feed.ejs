            <div class="div-card">
                <% for (let i = 0; i < servicos.length; i++) { %>
                    <div class="card" style="width: 18rem;">
                        <h2 class="titulo-card"><%=servicos[i].bairro%></h2>
                        <img src="../public/images/predio2.jpg" class="card-img-top" alt="..." style="border-radius:0%;">
                        <div class="card-body">
                            <h2><%=servicos[i].nome%></h2>
                            <hr class="card_line">
                            <div class="card_date">
                                <p class="card-text">Inicio: <%=servicos[i].data_abertura%></p>
                                <p class="card-text">Prazo: <%=servicos[i].data_finadlizacao%></p>
                            </div>
                            <div id = "card_b">
                                <a class="link" href="/servico/editarServico?id_servico=<%=servicos[i].id_servico%>">
                                <button class="btn_card">
                                    <img  src="../public/images/editar.png">
                                </button>
                                </a>
                                <a href="/servico/deletarServico?id_servico=<%=servicos[i].id_servico%>">
                                <button class="btn_card">
                                    <img  src="../public/images/rejeitado.png">
                                </button>
                                </a>
                            </div>
                        </div>
                    </div>
                <% } %>
            </div>
